public class ArrayOperations {
  
  public static void main(String[] args){
  /* ADD YOUR CODE HERE */
  double [] array = {5.6,1.2,8.3};
  
  double sum;
  for(int i = 0; i < array.length; i++)
    {
      sum += array[i];
    }

    System.out.println("The sum is " + sum);
  
  
  }

}
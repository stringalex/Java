import java.util.Scanner;

public class starexer
{
  public static void main(String[] args)
  {
    
    
    //Scanner keyboard = new Scanner(System.in);
   // int n = keyboard.nextInt();
    for(int i = 1; i <= 2; i++)
      {
      for(int j = 0; j < 3; j++) {
        System.out.print(" ");
      }
          for(int k = 0; k < 4;k++)
              System.out.print("*");
          
          System.out.println();
      }
  }
}
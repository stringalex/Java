import java.util.Scanner;

public class AllPositive {
    public static void main(String[] args) {
        Scanner keyboard = new Scanner(System.in);
        
    //enter #
    
    int n = keyboard.nextInt();
    
    //validation loop
    
    while( n <= 1) {
          n = keyboard.nextInt();
          if( n > 100){
              System.out.println(n);
              
          }
    }
   
    
    }
        
    }
}